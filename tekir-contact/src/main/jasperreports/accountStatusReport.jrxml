	<queryString>
		<![CDATA[SELECT tat.CODE, tc.NAME, 
SUM(CASE WHEN tat.DEBIT = true THEN tat.LOCAL_AMOUNT ELSE 0 END) DEBIT, 
SUM(CASE WHEN tat.DEBIT = false THEN tat.LOCAL_AMOUNT ELSE 0 END) RECEIVABLE
FROM TCC_ACCOUNT_TXN tat 
INNER JOIN TCC_CONTACT as tc on tat.ACCOUNT_ID = tc.ID
WHERE 
tat.TXNDATE < $P{DATE} 
AND (tc.CAT_ID = $P{CONTACT_CAT_ID} OR $P{CONTACT_CAT_ID}=0) 
AND (tc.TYPE_ID = $P{CORP_TYPE_ID} OR $P{CORP_TYPE_ID}=0) 
AND (tc.INDUSTRY_ID = $P{INDUSTRY_ID}  OR $P{INDUSTRY_ID}=0)  
AND (tc.TERRITORY_ID = $P{TERRITORY_ID} OR $P{TERRITORY_ID}=0)
AND tc.NAME LIKE CONCAT('%',$P{NAME},'%')
AND tc.CODE LIKE CONCAT('%',$P{CODE},'%')
GROUP BY tat.ACCOUNT_ID]]>
	</queryString>
