	<queryString>
		<![CDATA[SELECT
acc.CODE, acc.NAME,
SUM(CASE WHEN txn.DEBIT=false THEN txn.LOCAL_AMOUNT ELSE 0 END) Debit,
SUM(CASE WHEN txn.DEBIT=true THEN txn.LOCAL_AMOUNT ELSE 0 END) Receivable,
SUM(CASE WHEN txn.DEBIT=false THEN txn.LOCAL_AMOUNT ELSE 0 END)-SUM(CASE WHEN txn.DEBIT=true THEN txn.LOCAL_AMOUNT ELSE 0 END) 
FROM TFN_FINANCE_TXN txn INNER JOIN TFN_FINANCE_ACC acc ON acc.ID=txn.ACCOUNT_ID
GROUP BY acc.ID]]>
	</queryString>
	<field name="CODE" class="java.lang.String"/>
	<field name="NAME" class="java.lang.String"/>
	<field name="Debit" class="java.math.BigDecimal"/>
	<field name="Receivable" class="java.math.BigDecimal"/>
	<field name="SUM(CASE WHEN txn.DEBIT=false THEN txn.LOCAL_AMOUNT ELSE 0 END)-SUM(CASE WHEN txn.DEBIT=true THEN txn.LOCAL_AMOUNT ELSE 0 END)" class="java.math.BigDecimal"/>
	<variable name="Sum" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{Receivable}.subtract( $F{Debit})]]></variableExpression>
	</variable>
